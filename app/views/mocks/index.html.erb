<p id="notice"><%= notice %></p>

<h1>Listing Mocks</h1>



<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div id = "container">
    </div>
    <% @mocks.each do |mock| %>
      <tr>
        <td><%= link_to 'Show', mock %></td>
        <td><%= link_to 'Edit', edit_mock_path(mock) %></td>
        <td><%= link_to 'Destroy', mock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mock', new_mock_path %>
